<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.myweatheralert" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Toasty">
        <param name="android-package" value="com.chariotsolutions.toast.plugin.ToastPlugin" />
    </feature>
    <name>myWeatherAlert</name>
    <description>
        Gather and display earthquake data live.
    </description>
    <author email="gsdhawan@sfu.ca" href="http://shdh.ca">
        Shawn Dhawan
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
